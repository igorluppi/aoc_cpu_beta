module freq_div10 (clk, clk_out);
	input clk;
	output reg clk_out = 0;
	reg [3:0] estado = 0;

	always@(negedge clk)
		begin
			case(estado)
				0: estado = 1;
				1: estado = 2;
				2: estado = 3;
				3: estado = 4;
				4: 
					begin 
						estado = 5;
						clk_out = 1;
					end
				5: estado = 6;
				6: estado = 7;
				7: estado = 8;
				8: estado = 9;
				default:
					begin 
						estado = 0;
						clk_out = 0;
					end
			endcase
		end

endmodule